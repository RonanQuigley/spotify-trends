{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/transform-runtime", {
      "regenerator" : true, 
      "polyfill" : true
    }]
  ],
  "env": {
    "testing" : {
      "plugins": [
        "babel-plugin-rewire"
      ]
    }
  },
  "sourceMaps": true,
  "retainLines": true
}
