  "scripts": {
    "build:back": "webpack -p --config webpack/back-end/webpack.prod.babel.js",
    "build:front": "webpack -p --config webpack/front-end/webpack.prod.babel.js",
    "build:all": "webpack -p --config webpack/full-stack/webpack.babel.js",
    "build:all:start": "npm-run-all -s build:all start:prod",
    "build:analyze": "cross-env ANALYZE=true npm-run-all build:all",
    "start:prod": "cross-env NODE_ENV=production DEBUG=false node ./dist/server.js",
    "start:dump-log": "npm run start -- > dump.txt",
    "start": "cross-env NODE_ENV=development node --max-old-space-size=8192 --inspect --require @babel/register --require dotenv/config ./src/index.js",
    "start:debug": "cross-env NODE_ENV=development DEBUG=true node --inspect --require @babel/register --require dotenv/config ./src/index.js",
    "watch:lint": "node node_modules/eslint-watch/bin/esw -w --fix",
    "test:front": "mocha-webpack --opts mocha.opts --webpack-config ./webpack/front-end/webpack.dev.js test/front-end/**/*.test.js",
    "test:common": "mocha-webpack --opts mocha.opts --webpack-config ./webpack/front-end/webpack.dev.js test/common/**/*.test.js",
    "test:back": "mocha-webpack --opts mocha.opts --webpack-config ./webpack/back-end/webpack.dev.js test/back-end/**/*.test.js",
    "test:all": "npm-run-all -p test:front test:common test:back",
    "coverage:front": "nyc --include-all-sources --color --reporter=lcov --reporter=text --report-dir ./coverage/front mocha-webpack --require mocha.setup.js --webpack-config ./webpack/front-end/webpack.dev.babel.js \"test/front-end/**/*.test.js\"",
    "coverage:back": "nyc --include-all-sources --color --reporter=lcov --reporter=text --report-dir ./coverage/back mocha-webpack --require mocha.setup.js --webpack-config ./webpack/back-end/webpack.dev.babel.js \"test/back-end/**/*.test.js\"",
    "coverage:all": "npm-run-all -p coverage:front coverage:back"
  },
  "nyc": {
    "include": [
      "src/**/*.(js)"
    ],
    "instrument": false,
    "sourceMap": false
