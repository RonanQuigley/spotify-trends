  "scripts": {
    "build:back": "webpack -p --config webpack/back-end/webpack.prod.babel.js",
    "build:front": "webpack -p --config webpack/front-end/webpack.prod.babel.js",
    "build:all": "webpack --config webpack/full-stack/webpack.babel.js",
    "build:all:start": "npm-run-all -s build:all start:prod",
    "start:prod": "cross-env NODE_ENV=production DEBUG=false node ./dist/server.js",
    "start": "cross-env NODE_ENV=development node --inspect --require @babel/register --require dotenv/config ./src/index.js",
    "watch:lint": "node node_modules/eslint-watch/bin/esw -w --fix",
    "test:front": "mocha-webpack --opts mocha.opts --webpack-config ./webpack/front-end/webpack.dev.js \"test/front-end/**/*.test.js\"",
    "test:back": "mocha-webpack --opts mocha.opts --webpack-config ./webpack/back-end/webpack.dev.js \"test/back-end/**/*.test.js\"",
    "test:all": "npm-run-all -p test:front test:back",
    "coverage:front": "nyc --color --reporter=lcov --reporter=text --report-dir ./coverage/front mocha-webpack --require mocha.setup.js --webpack-config ./webpack/front-end/webpack.dev.babel.js \"test/front-end/**/*.test.js\"",
    "coverage:back": "nyc --color --reporter=lcov --reporter=text --report-dir ./coverage/back mocha-webpack --require mocha.setup.js --webpack-config ./webpack/back-end/webpack.dev.babel.js \"test/back-end/**/*.test.js\"",
    "coverage:all": "npm-run-all -p coverage:front coverage:back",
    "mocha-debug": "mocha --opts mocha.opts ./test/dud.test.js"
  },
